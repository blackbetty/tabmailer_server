{
  "name": "tabmailer",
  "version": "0.0.1",
  "description": "An extension that saves tabs youve been hoarding and randomly emails them to you",
  "main": "app.js",
  "repository": "https://gitlab.com/tabmailer/tabmailer_server.git",
  "scripts": {
    "test": "test",
    "start": "node app.js",
    "startdev": "NODE_ENV=development nodemon --ignore 'logs/*' --ignore 'pages/*' app.js",
    "debug": "NODE_ENV=development node --debug app.js",
    "build": "cross-env NODE_ENV=development webpack-dev-server --open --hot"
  },
  "keywords": [
    "tabs",
    "email",
    "hoarding"
  ],
  "engines": {
    "node": "8.8.1"
  },
  "author": "Dan Golant",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/datastore": "^1.0.7",
    "body-parser": "^1.17.2",
    "celebrate": "^5.0.1",
    "cors": "^2.8.4",
    "cron": "^1.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-https-redirect": "^1.0.0",
    "google-auth-library": "^0.10.0",
    "grpc": "^1.6.0",
    "install": "^0.10.1",
    "lodash": "^4.17.4",
    "mailgen": "^2.0.2",
    "moment": "^2.19.1",
    "node-fqdn": "^1.1.1",
    "nodemailer": "^4.0.1",
    "nodemailer-mailgun-transport": "^1.3.5",
    "npm": "^5.3.0",
    "object-hash": "^1.1.8",
    "os": "^0.1.1",
    "pem": "^1.9.7",
    "request": "^2.81.0",
    "sync": "^0.2.5",
    "synchronize": "^2.0.0",
    "underscore": "^1.8.3",
    "vue": "^2.4.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
